<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controllers.ModifierVoitureController">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="Marque:" />
        <TextField fx:id="tfMarque" layoutX="150.0" layoutY="50.0" />

        <Label layoutX="50.0" layoutY="100.0" text="Modèle:" />
        <TextField fx:id="tfModele" layoutX="150.0" layoutY="100.0" />

        <Label layoutX="50.0" layoutY="150.0" text="Année:" />
        <TextField fx:id="tfAnnee" layoutX="150.0" layoutY="150.0" />

        <Label layoutX="50.0" layoutY="200.0" text="Prix par jour:" />
        <TextField fx:id="tfPrixParJour" layoutX="150.0" layoutY="200.0" />

        <Label layoutX="50.0" layoutY="250.0" text="Disponible:" />
        <TextField fx:id="tfDisponible" layoutX="150.0" layoutY="250.0" />

        <Button layoutX="250.0" layoutY="300.0" text="Modifier" onAction="#modifierVoiture" />
    </children>
</AnchorPane>