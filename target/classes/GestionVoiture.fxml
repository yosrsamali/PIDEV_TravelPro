            <Font name="Arial Bold" size="25.0" />
         </font>
      </Text>

      <!-- Button Ajouter -->
      <Button layoutX="463.0" layoutY="501.0" mnemonicParsing="false" onAction="#ajouterVoiture" prefHeight="30.0" prefWidth="111.0" text="Ajouter" />

