      <!-- TextField Prix par jour -->
      <TextField fx:id="tfPrixParJour" alignment="TOP_CENTER" layoutX="443.0" layoutY="391.0" nodeOrientation="LEFT_TO_RIGHT" promptText="ex: 250.0">
