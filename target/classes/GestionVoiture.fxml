            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>

      <!-- Label Prix par jour -->
      <Label layoutX="338.0" layoutY="393.0" prefHeight="30.0" prefWidth="91.0" text="Prix par jour:" textAlignment="CENTER">
