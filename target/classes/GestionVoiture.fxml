            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>

      <!-- Label Disponibilité -->
      <Label layoutX="338.0" layoutY="449.0" prefHeight="30.0" prefWidth="91.0" text="Disponibilité:">
