      <!-- Titre Ajout du Voiture -->
      <Text layoutX="467.0" layoutY="199.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ajout du Voiture" textAlignment="CENTER" wrappingWidth="213.33673095703125">
