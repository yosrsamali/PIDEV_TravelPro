            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>

      <!-- Label Modele -->
      <Label layoutX="338.0" layoutY="281.0" prefHeight="30.0" prefWidth="91.0" text="Modele:">
         <font>
            <Font name="System Bold" size="14.0" />
