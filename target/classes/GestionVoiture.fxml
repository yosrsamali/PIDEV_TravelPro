            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>

      <!-- Label Annee -->
      <Label layoutX="338.0" layoutY="335.0" prefHeight="30.0" prefWidth="72.0" text="Annee:">
         <font>
            <Font name="System Bold" size="14.0" />
